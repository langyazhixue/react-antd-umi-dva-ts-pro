{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint-config-umi",
  "rules": {
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "none"
    }]
  }
}
