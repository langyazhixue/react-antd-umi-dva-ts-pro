{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react-hooks"
  ],
  "extends": "eslint-config-umi",
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react/no-typos":0,
    "react-hooks/exhaustive-deps": "warn",
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "none"
    }]
  }
}
